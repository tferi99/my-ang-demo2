{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/my-ang-demo2-server/src",
  "projects": {
    "my-ang-demo2-server": {
      "type": "application",
      "root": "apps/my-ang-demo2-server",
      "entryFile": "main",
      "sourceRoot": "apps/my-ang-demo2-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my-ang-demo2-server/tsconfig.app.json"
      }
    },
    "my-ang-demo2-lib": {
      "type": "library",
      "root": "libs/my-ang-demo2-lib",
      "entryFile": "index",
      "sourceRoot": "libs/my-ang-demo2-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/my-ang-demo2-lib/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/my-ang-demo2-server/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/my-ang-demo2-server"
}
